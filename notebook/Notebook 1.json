						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					" customer_df = df.select(\"customerid\", \"customerdetails\")\r\n",
					" display(customer_df)"
				],
				"execution_count": 2
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
